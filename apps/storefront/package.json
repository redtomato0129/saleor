{
  "name": "storefront",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "debug": "NODE_OPTIONS='--inspect' next dev --port 3000",
    "dev": "next dev --port 3000",
    "build": "next build",
    "postbuild": "next-sitemap",
    "translations": "formatjs extract './{components,pages}/**/*.{ts,tsx}' --out-file temp.json --flatten --id-interpolation-pattern '[sha512:contenthash:base64:6]' && formatjs compile 'temp.json' --out-file locale/en-US.json && rm ./temp.json",
    "analyze-build": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint --dir .",
    "lint:staged": "next lint --dir . --fix --quiet",
    "check-types": "tsc --noEmit",
    "generate": "dotenv -c -- graphql-codegen --config codegen.yml -w",
    "paths": "pathpida --ignorePath ../../.gitignore",
    "clean": "rm -rf .next .turbo"
  },
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@formatjs/swc-plugin": "^1.4.0",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.1.15",
    "@headlessui/react": "^1.6.0",
    "@heroicons/react": "^1.0.6",
    "@saleor/sdk": "^0.4.4",
    "@saleor/ui-kit": "workspace:*",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.26.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "clsx": "1.1.1",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.1.0",
    "editorjs-blocks-react-renderer": "^1.2.4",
    "graphql": "16.3.0",
    "graphql-tag": "^2.12.6",
    "next": "12.3.1",
    "next-seo": "^5.5.0",
    "next-sitemap": "^2.4.3",
    "next-usequerystate": "^1.7.2",
    "nextjs-progressbar": "^0.0.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.29.0",
    "react-intl": "^5.24.6",
    "react-use": "^17.3.2",
    "tailwind-scrollbar-hide": "^1.1.7",
    "url-join": "4.0.1"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.8.2",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-react-apollo": "3.2.11",
    "@next/bundle-analyzer": "^12.1.4",
    "@svgr/webpack": "^6.2.1",
    "@types/node": "^16.11.22",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/url-join": "4.0.1",
    "autoprefixer": "10.4.2",
    "eslint": "^8.22.0",
    "eslint-config-checkout": "workspace:*",
    "pathpida": "^0.18.1",
    "postcss": "8.4.12",
    "prettier": "2.6.2",
    "react-hook-form": "^7.29.0",
    "react-use": "^17.3.2",
    "tailwindcss": "3.0.23",
    "typescript": "4.7.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": "^14 || ^16 || ^18",
    "pnpm": ">=7.7.1"
  },
  "packageManager": "pnpm@7.11.0"
}

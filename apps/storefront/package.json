{
  "name": "storefront",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "debug": "NODE_OPTIONS='--inspect' next dev --port 3000",
    "dev": "next dev --port 3000",
    "build": "next build",
    "postbuild": "next-sitemap",
    "translations": "formatjs extract './{components,pages}/**/*.{ts,tsx}' --out-file temp.json --flatten --id-interpolation-pattern '[sha512:contenthash:base64:6]' && formatjs compile 'temp.json' --out-file locale/en-US.json && rm ./temp.json",
    "analyze-build": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint --dir .",
    "lint:staged": "next lint --dir . --fix --quiet",
    "check-types": "tsc --noEmit",
    "generate": "dotenv -c -- graphql-codegen --config codegen.yml -w",
    "paths": "pathpida --ignorePath ../../.gitignore",
    "clean": "rm -rf .next .turbo"
  },
  "dependencies": {
    "@apollo/client": "3.7.1",
    "@formatjs/swc-plugin": "1.4.14",
    "@graphql-codegen/typescript-apollo-client-helpers": "2.2.5",
    "@headlessui/react": "1.7.3",
    "@heroicons/react": "1.0.6",
    "@saleor/ui-kit": "workspace:*",
    "@stripe/react-stripe-js": "1.14.0",
    "@stripe/stripe-js": "1.42.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.7",
    "clsx": "1.2.1",
    "dotenv": "16.0.3",
    "dotenv-cli": "6.0.0",
    "editorjs-blocks-react-renderer": "1.3.0",
    "graphql": "16.6.0",
    "graphql-tag": "2.12.6",
    "next": "13.0.4",
    "next-seo": "5.8.0",
    "next-sitemap": "3.1.29",
    "next-usequerystate": "1.7.2",
    "nextjs-progressbar": "0.0.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.38.0",
    "react-intl": "6.2.1",
    "react-use": "17.4.0",
    "tailwind-scrollbar-hide": "1.1.7",
    "ts-invariant": "0.10.3",
    "url-join": "4.0.1"
  },
  "devDependencies": {
    "@formatjs/cli": "5.1.3",
    "@graphql-codegen/add": "3.2.1",
    "@graphql-codegen/cli": "2.13.7",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.8.0",
    "@graphql-codegen/typescript-operations": "2.5.5",
    "@graphql-codegen/typescript-react-apollo": "3.3.5",
    "@next/bundle-analyzer": "13.0.0",
    "@svgr/webpack": "6.5.0",
    "@types/node": "16.11.22",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.23",
    "@types/react-dom": "18.0.7",
    "@types/url-join": "4.0.1",
    "autoprefixer": "10.4.12",
    "eslint": "8.26.0",
    "eslint-config-checkout": "workspace:*",
    "pathpida": "0.18.1",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "react-hook-form": "7.29.0",
    "react-use": "17.3.2",
    "tailwindcss": "3.2.1",
    "typescript": "4.8.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": "14 || ^16 || ^18",
    "pnpm": ">=7.7.1"
  },
  "packageManager": "pnpm@7.11.0"
}

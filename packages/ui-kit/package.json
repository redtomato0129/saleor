{
  "name": "@saleor/ui-kit",
  "version": "0.0.0",
  "description": "UI kit for Saleor Checkout and React Storefront",
  "author": "Saleor Commerce",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "license": "BSD-3-Clause",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "NODE_OPTIONS=--experimental-json-modules NODE_ENV=production rollup -c",
    "dev": "NODE_OPTIONS=--experimental-json-modules rollup -c -w",
    "lint": "eslint .",
    "lint:staged": "eslint . --fix --quiet",
    "check-types": "tsc",
    "clean": "rm -rf .next .turbo dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release": "release-it --npm.pnpm"
  },
  "peerDependencies": {
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "resolutions": {
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@rollup/plugin-commonjs": "23.0.2",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "9.0.2",
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-interactions": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-webpack5": "6.5.13",
    "@storybook/manager-webpack5": "6.5.13",
    "@storybook/react": "6.5.13",
    "@storybook/testing-library": "0.0.13",
    "@types/node": "16.11.22",
    "@types/react": "18.0.23",
    "@types/react-dom": "18.0.7",
    "autoprefixer": "10.4.12",
    "babel-loader": "8.2.5",
    "css-loader": "6.7.1",
    "css-modules-typescript-loader": "4.0.1",
    "eslint-config-checkout": "workspace:*",
    "postcss": "8.4.18",
    "postcss-loader": "7.0.1",
    "react": "18.2.0",
    "react-docgen-typescript": "2.2.2",
    "react-dom": "18.2.0",
    "release-it": "15.5.0",
    "rollup-plugin-dts": "5.0.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-terser": "7.0.2",
    "style-loader": "3.3.1",
    "tailwindcss": "3.2.1",
    "tsconfig": "workspace:*",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@headlessui/react": "1.7.3",
    "clsx": "1.2.1",
    "rollup": "3.2.3",
    "tslib": "2.4.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0",
    "pnpm": ">=7.7.1"
  },
  "packageManager": "pnpm@7.11.0"

{
  "name": "@saleor/checkout-storefront",
  "version": "0.1.0",
  "private": true,
  "module": "dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "pnpm run i18n && concurrently --kill-others 'next dev -p 3002' 'NODE_OPTIONS=--experimental-json-modules rollup -w -c'",
    "build": "pnpm run i18n && NODE_OPTIONS=--experimental-json-modules NODE_ENV=production rollup -c",
    "generate": "DOTENV_CONFIG_PATH=.env graphql-codegen -r env-vars --config codegen.yml",
    "check-types": "tsc --noEmit",
    "check-types:dev": "tsc --noEmit --watch",
    "lint": "eslint .",
    "lint:staged": "eslint . --fix --quiet",
    "test": "jest",
    "test:dev": "jest --watch",
    "clean": "rm -rf .turbo .next .rollup.cache dist",
    "i18n:extract": "formatjs extract 'src/**/*.{js,jsx,ts,tsx}' --format transifex --id-interpolation-pattern '[folder]/[name]/[sha512:contenthash:base64:6]' --out-file content/locales/en-US.json",
    "i18n:compile": "formatjs compile-folder --ast --format transifex content/locales content/compiled-locales",
    "i18n": "pnpm run i18n:extract && pnpm run i18n:compile"
  },
  "resolutions": {
    "graphql": "15.8.0"
  },
  "peerDependencies": {
    "graphql": "15.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "overrides": {
    "@types/react": "18.0.15"
  },
  "dependencies": {
    "@adyen/adyen-web": "5.28.2",
    "@adyen/api-library": "12.0.0",
    "@apollo/client": "3.7.1",
    "@headlessui/react": "1.7.3",
    "@react-types/button": "3.6.2",
    "@saleor/ui-kit": "workspace:*",
    "checkout-common": "workspace:*",
    "@stripe/react-stripe-js": "1.14.0",
    "@stripe/stripe-js": "1.42.0",
    "clsx": "1.2.1",
    "concurrently": "7.5.0",
    "formik": "^2.2.9",
    "hex-rgb": "5.0.0",
    "libphonenumber-js": "^1.10.14",
    "lodash-es": "4.17.21",
    "query-string": "7.1.1",
    "react": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-intl": "6.2.1",
    "react-test-renderer": "18.2.0",
    "react-toastify": "9.0.8",
    "tailwindcss": "3.2.1",
    "ts-invariant": "0.10.3",
    "url-join": "4.0.1",
    "urql": "3.0.3",
    "yup": "1.0.0-beta.7",
    "zustand": "^4.1.4"
  },
  "devDependencies": {
    "@formatjs/cli": "5.1.3",
    "@graphql-codegen/add": "3.2.1",
    "@graphql-codegen/cli": "2.13.7",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.8.0",
    "@graphql-codegen/typescript-operations": "2.5.5",
    "@graphql-codegen/typescript-urql": "3.7.2",
    "@graphql-codegen/urql-introspection": "2.2.1",
    "@rollup/plugin-commonjs": "23.0.2",
    "@rollup/plugin-image": "3.0.1",
    "@rollup/plugin-json": "5.0.1",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "9.0.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "27.5.2",
    "@types/lodash-es": "4.17.6",
    "@types/node": "16.11.22",
    "@types/react": "18.0.23",
    "@types/react-dom": "18.0.7",
    "@types/url-join": "4.0.1",
    "@types/webpack-env": "1.18.0",
    "autoprefixer": "10.4.12",
    "env-vars": "workspace:*",
    "eslint": "8.26.0",
    "eslint-config-checkout": "workspace:*",
    "eslint-plugin-formatjs": "4.3.4",
    "graphql-tag": "2.12.6",
    "jest": "27.5.1",
    "next": "13.0.4",
    "postcss": "8.4.18",

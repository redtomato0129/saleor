{
  "name": "@saleor/checkout-storefront",
  "version": "0.1.0",
  "private": true,
  "module": "dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "concurrently --kill-others 'next dev -p 3002' 'rollup -w -c'",
    "build": "NODE_ENV=production rollup -c",
    "generate": "DOTENV_CONFIG_PATH=.env graphql-codegen -r env-vars --config codegen.yml",
    "check-types": "tsc --noEmit",
    "check-types:dev": "tsc --noEmit --watch",
    "lint": "eslint .",
    "lint:staged": "eslint . --fix --quiet && prettier --write .",
    "test": "jest",
    "test:dev": "jest --watch",
    "clean": "rm -rf .turbo .next .rollup.cache dist",
    "extract:i18n": "formatjs extract 'src/**/*.{js,jsx,ts,tsx}' --format transifex --id-interpolation-pattern '[folder]/[name]/[sha512:contenthash:base64:6]' --out-file content/locales/en-US.json",
    "compile:i18n": "formatjs compile-folder --ast --format transifex content/locales content/compiled-locales",
    "i18n": "npm run extract:i18n && npm run compile:i18n"
  },
  "resolutions": {
    "graphql": "15.8.0"
  },
  "peerDependencies": {
    "graphql": "15.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "overrides": {
    "@types/react": "^18.0.15"
  },
  "dependencies": {
    "@adyen/adyen-web": "5.26.0",
    "@adyen/api-library": "12.0.0",
    "@apollo/client": "^3.5.10",
    "@headlessui/react": "^1.6.0",
    "@hookform/error-message": "^2.0.0",
    "@react-types/button": "^3.4.1",
    "@saleor/sdk": "^0.4.3",
    "@saleor/ui-kit": "workspace:*",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.26.0",
    "checkout-common": "workspace:*",
    "clsx": "^1.1.1",
    "concurrently": "7.3.0",
    "hex-rgb": "^5.0.0",
    "lodash-es": "^4.17.21",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.34.2",
    "react-intl": "^6.1.1",
    "react-test-renderer": "^18.2.0",
    "react-toastify": "^9.0.4",
    "tailwindcss": "^3.0.18",
    "ts-invariant": "0.10.3",
    "url-join": "4.0.1",
    "urql": "^2.2.3",
    "yup": "1.0.0-beta.7"
  },
  "devDependencies": {
    "@formatjs/cli": "5.1.0",
    "@graphql-codegen/add": "^3.2.0",
    "@graphql-codegen/cli": "2.8.1",
    "@graphql-codegen/introspection": "2.2.0",
    "@graphql-codegen/typescript": "2.7.1",
    "@graphql-codegen/typescript-operations": "2.5.1",
    "@graphql-codegen/typescript-urql": "^3.6.1",
    "@graphql-codegen/urql-introspection": "2.2.0",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-image": "2.1.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.22",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/url-join": "4.0.1",
    "@types/webpack-env": "^1.16.3",
    "autoprefixer": "9.8.6",
    "env-vars": "workspace:*",
    "eslint": "^8.22.0",
    "eslint-config-checkout": "workspace:*",
    "eslint-plugin-formatjs": "^4.2.2",
    "graphql-tag": "^2.12.6",
    "jest": "^27.5.1",
    "next": "12.3.1",
    "postcss": "^8.4.6",
    "postcss-import": "14.1.0",
    "prettier": "^2.5.1",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.1.5",
    "tsconfig": "workspace:*",
    "tslib": "^2.4.0",
    "typescript": "4.7.4",
    "web-vitals": "^2.1.4",
    "wonka": "^6.0.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0",
    "pnpm": ">=7.7.1"
  },
  "packageManager": "pnpm@7.11.0"
}

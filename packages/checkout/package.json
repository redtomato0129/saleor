{
  "name": "@saleor/checkout",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "BSD-3-Clause",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "dev": "PORT=3000 tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "eslint --max-warnings=0 \"src/**/*.@(tsx|ts|jsx|js)\" --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "config": "*",
    "react": "^17.0.2",
    "tsconfig": "*",
    "tsup": "^5.11.11",
    "typescript": "^4.5.3"
  }
}

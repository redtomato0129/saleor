{
  "name": "saleor-storefront",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "generate": "turbo run generate",
    "dev": "env-cmd -f .env env-cmd -f .env.local turbo run dev --parallel",
    "lint": "turbo run lint",
    "lint:staged": "env-cmd -f .env env-cmd -f .env.local turbo run lint:staged",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "turbo": "^1.3.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.5.0"
}
